{
  "entryPoints": [
    "./packages/api/src",
    "./packages/web/src",
    "./packages/shared"
  ],
  "entryPointStrategy": "expand",
  "out": "./docs/api",
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "API",
    "Web",
    "Shared",
    "*"
  ],
  "plugin": [],
  "theme": "default",
  "name": "NeuralChat API Documentation",
  "readme": "./README.md"
}